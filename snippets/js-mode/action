const ${1:$(upcase (s-snake-case yas-text))} = '${1:$(upcase (s-snake-case yas-text))}';

export const ${1:actionName} = (${2:args}) => ({
  type: '${1:$(upcase (s-snake-case yas-text))}',
  payload: {
    $0
  },
});
