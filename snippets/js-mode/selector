const ${1:...}Selector = createSelector(
  (state) => state.${2:...}.allIds,
  (state) => state.$2.byId,
  (
    allIds: string[],
    byId: {
      [string]: ${3:...}
    }
  ) => allIds.map((id) => byId[id])
);$0
