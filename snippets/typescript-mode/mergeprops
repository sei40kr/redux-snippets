# name : Redux mergeProps
# contributor : Seong Yong-ju <sei40kr@gmail.com>
# key: mergeprops
# --

const mergeProps = (stateProps: IStateProps, dispatchProps: IDispatchProps, ownProps: {}): I`(redux-snippets//container-component-name)`Props => ({
  ...stateProps,
  ...dispatchProps,
  ...ownProps,
  $0
});
